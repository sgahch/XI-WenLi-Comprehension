<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.TEvaluationSystemMapper">
    
    <resultMap type="TEvaluationSystem" id="TEvaluationSystemResult">
        <result property="id"    column="id"    />
        <result property="systemName"    column="system_name"    />
        <result property="academicYear"    column="academic_year"    />
        <result property="semester"    column="semester"    />
        <result property="description"    column="description"    />
        <result property="status"    column="status"    />
        <result property="startTime"    column="start_time"    />
        <result property="endTime"    column="end_time"    />
        <result property="creatorId"    column="creator_id"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectTEvaluationSystemVo">
        select id, system_name, academic_year, semester, description, status, start_time, end_time, creator_id, create_time, update_time from t_evaluation_system
    </sql>

    <select id="selectTEvaluationSystemList" parameterType="TEvaluationSystem" resultMap="TEvaluationSystemResult">
        <include refid="selectTEvaluationSystemVo"/>
        <where>  
            <if test="systemName != null  and systemName != ''"> and system_name like concat('%', #{systemName}, '%')</if>
            <if test="academicYear != null  and academicYear != ''"> and academic_year = #{academicYear}</if>
            <if test="semester != null "> and semester = #{semester}</if>
            <if test="description != null  and description != ''"> and description = #{description}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="startTime != null "> and start_time = #{startTime}</if>
            <if test="endTime != null "> and end_time = #{endTime}</if>
            <if test="creatorId != null "> and creator_id = #{creatorId}</if>
        </where>
    </select>
    
    <select id="selectTEvaluationSystemById" parameterType="Long" resultMap="TEvaluationSystemResult">
        <include refid="selectTEvaluationSystemVo"/>
        where id = #{id}
    </select>

    <insert id="insertTEvaluationSystem" parameterType="TEvaluationSystem" useGeneratedKeys="true" keyProperty="id">
        insert into t_evaluation_system
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="systemName != null and systemName != ''">system_name,</if>
            <if test="academicYear != null and academicYear != ''">academic_year,</if>
            <if test="semester != null">semester,</if>
            <if test="description != null">description,</if>
            <if test="status != null">status,</if>
            <if test="startTime != null">start_time,</if>
            <if test="endTime != null">end_time,</if>
            <if test="creatorId != null">creator_id,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="systemName != null and systemName != ''">#{systemName},</if>
            <if test="academicYear != null and academicYear != ''">#{academicYear},</if>
            <if test="semester != null">#{semester},</if>
            <if test="description != null">#{description},</if>
            <if test="status != null">#{status},</if>
            <if test="startTime != null">#{startTime},</if>
            <if test="endTime != null">#{endTime},</if>
            <if test="creatorId != null">#{creatorId},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateTEvaluationSystem" parameterType="TEvaluationSystem">
        update t_evaluation_system
        <trim prefix="SET" suffixOverrides=",">
            <if test="systemName != null and systemName != ''">system_name = #{systemName},</if>
            <if test="academicYear != null and academicYear != ''">academic_year = #{academicYear},</if>
            <if test="semester != null">semester = #{semester},</if>
            <if test="description != null">description = #{description},</if>
            <if test="status != null">status = #{status},</if>
            <if test="startTime != null">start_time = #{startTime},</if>
            <if test="endTime != null">end_time = #{endTime},</if>
            <if test="creatorId != null">creator_id = #{creatorId},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteTEvaluationSystemById" parameterType="Long">
        delete from t_evaluation_system where id = #{id}
    </delete>

    <delete id="deleteTEvaluationSystemByIds" parameterType="String">
        delete from t_evaluation_system where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>